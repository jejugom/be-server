<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.scoula.gift.mapper.StrategyMapper">

    <select id="findAll" resultType="org.scoula.gift.domain.StrategyVo">
        SELECT
            strategy_id,
            strategy_code,
            rule_category,
            message,
            base_weight,
            priority_level
        FROM
            strategy
    </select>

</mapper>
